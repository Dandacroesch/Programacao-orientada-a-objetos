public class Operadores {
	public static void main(String args []) {
		//declaração e inicialização de variáveis 
		int x = 10;
		int y = 3;
		//várias operações com as variáveis 
		
        System.out.println("x+y ="+(x+y));
        System.out.println("x*y ="+(x*y));
        System.out.println("x-y ="+(x-y));
        System.out.println("x/y ="+(x/y));
	}
}
